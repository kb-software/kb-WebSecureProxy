COURIER = [ESC] 6C 02
UL_ON = [ESC] 2D 01
UL_OFF = [ESC] 2D 00
TOPMARGIN = [ESC] \"(V\" 02 00 ByteDez (TopMargin1440/4) ByteDez (TopMargin1440/4/256)
LEFTMARGIN = [ESC] \"l\" ByteDez (LeftMargin)

