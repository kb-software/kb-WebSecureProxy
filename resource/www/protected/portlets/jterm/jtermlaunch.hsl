<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.1//EN" "http://www.w3.org/TR/xhtml11/DTD/xhtml11.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <xsl:call-template name="html-header"/>
    <script>
        function confirmExit(){
            return '<xsl:value-of select="lang/html/welcome/jterm/leavepage"/>';
        }

        window.onbeforeunload = confirmExit;
    </script>
</head>
<body>
    <div id="page-outer" class="wide-page logo-right">
        <xsl:call-template name="header"/>
		<div id="page-area">
			<!-- page content -->
            <a id="logo"> 
                <xsl:attribute name="href"><xsl:value-of select="user/welcomesite"/></xsl:attribute>
                <xsl:attribute name="title"><xsl:value-of select="lang/html/all/btn-home"/></xsl:attribute>
            </a>
            <div id="page-inner">
                <object type="application/x-java-applet" width="100%" height="400">
                    <param name="codebase" value="/public/lib" />
                    <param name="code" value="hob.lau2.JLaunchApplet.class" />
                    <param name="archive" value="JLaunch2-J2.jar" />
                    <param name="base_product_id" value="102" />
                    <param name="startup" value="startup.xml">
                    <param name="xxwebprofile" value="Y">
                    <param name="xxuserhome" value="C:\Program Files\myHomeDir">
                    <param name="xxmultihome" value="Y">
                    <param name="xxhidebtn" value="Y">
                    <param name="image" value="images/splashHOB.gif">
                    <param name="progressbar" value="true">
                    <param name="boxmessage">
                        <xsl:attribute name="value"><xsl:value-of select="lang/html/welcome/jterm/loading"/></xsl:attribute>
                    </param>
                    <param name="draggable" value="true">
                    <param name="java_arguments" value="-Dsun.java2d.noddraw=true -Xmx512m">
                    <param name="username">
                        <xsl:attribute name="value"><xsl:value-of select="user/hsocks-id"/></xsl:attribute>
                    </param>
                    <param name="hpw">
                        <xsl:attribute name="value"><xsl:value-of select="user/hsocks-sticket"/></xsl:attribute>
                    </param>
                    <param name="rd_vpn" value="Y">
                    <param name="language">
                        <xsl:attribute name="value"><xsl:value-of select="user/language"/></xsl:attribute>
                    </param>
                    <br>
                    <a href="/protected/portlets/jterm/jlaunch.jnlp">
                        <!-- <xsl:value-of select="lang/html/welcome/jterm/jnlp"/> -->
                        Start HOBLink J-Term (Java Web Start)
                    </a>
                    <!-- <xsl:value-of select="lang/html/all/need-java"/> -->
                    <br>
                </object>                
            </div>
		</div>
        <div id="footer">
        </div>
    </div>
</body>
</html>

<xsl:include href="/public/template_header.hsl" />
